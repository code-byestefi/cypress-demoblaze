{
  "name": "cypress-demoblaze",
  "version": "1.0.0",
  "description": "Repositorio destinado a dise√±o de un testing framework",
  "main": "index.js",
  "scripts": {
    "clean-report": "rimraf ./allure-report && rimraf allure-results",
    "run-test": "cypress run --headed --env allure=true",
    "generate-report": "allure generate allure-results -o allure-report --clean",
    "open-report": "allure open",
    "test": "npm run clean-report && npm run run-test && npm run generate-report && npm run open-report"
  },
